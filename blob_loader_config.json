{
    "storageAccounts": [
        {
            "name": "storageAccount1",
            "authKey": "authKey1"
        },
        {
            "name": "storageAccount2",
            "authKey": "authKey2"
        },
        {
            "name": "storageAccount3",
            "authKey": "authKey3"
        }
    ],
    "containers": [
        {
            "name": "container1",
            "regex": "regex1"
        },
        {
            "name": "container2",
            "regex": "regex2"
        },
        {
            "name": "container3",
            "regex": "regex3"
        }
    ],
    "stages": [
        {
            "name": "timer",
            "folderName": "timerFolder",
            "threadCount": 1
        },
        {
            "name": "gatherinfo",
            "folderName": "gatherInfoFolder",
            "threadCount": 1
        },
        {
            "name": "loader",
            "folderName": "loadFolder",
            "threadCount": 5
        },
        {
            "name": "staging",
            "folderName": "stagingFolder",
            "threadCount": 3
        },
        {
            "name": "completed",
            "folderName": "completedFolder",
            "threadCount": 2
        }
    ],
    "maintenance": {
        "minDiskFreePercent": 10,
        "dataFolder": "/path/to/mainDataFolder",
        "defaultLagDuration": 10,
        "defaultOldThreshold": 86400,
        "TimerThreadWait": 2
    }
}
